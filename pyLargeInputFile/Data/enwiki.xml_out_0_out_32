  <page>
    <title>Anaïs Nin</title>
    <id>2919</id>
    <revision>
      <id>430961804</id>
      <timestamp>2011-05-26T04:42:15Z</timestamp>
      <contributor>
        <username>David R. Ingham</username>
        <id>300162</id>
