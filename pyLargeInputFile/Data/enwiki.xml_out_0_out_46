| style=&quot;text-align:center;&quot;| Я я || style=&quot;text-align:center;&quot;| Ja ja&lt;sup&gt;1&lt;/sup&gt; || style=&quot;text-align:center;&quot;| Ya ya || style=&quot;text-align:center;&quot;| {{IPA|/ja/}} ||я
