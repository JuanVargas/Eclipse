| publisher = [[Princeton University]] and the [[Institute for Advanced Study]]
| country = United States
| frequency = Bimonthly
| history = 1874–present
| openaccess = 
| impact = 3.447
| impact-year = 2008
| website = http://www.math.princeton.edu/~annals/
| link1 = 
| link1-name = 
| link2 = 
